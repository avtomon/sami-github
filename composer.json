{
    "name": "avtomon/sami-github",
    "description": "A Sami theme for GitHub Markdown formatted documentation. (this is a fork of phine/sami-github)",
    "keywords": ["sami", "github"],
    "homepage": "https://github.com/ppetermann/sami-github",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Avtomonov",
            "email": "avtomon86@gmail.com"
        },
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io"
        },
        {
            "name": "Peter Petermann",
            "email": "ppetermann80@googlemail.com",
            "homepage": "https://avtomon.eu"
        }
    ],
    "support": {
        "issues": "https://github.com/avtomon/sami-github/issues"
    },
    "require-dev": {
        "sami/sami": "~1.1"
    },
    "autoload": {
        "psr-0": {
            "Phine\\Sami\\GitHub": "test/source"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
